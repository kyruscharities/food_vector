%section
  .col-md-12
    %p#notice= notice

    %p
      %b Name:
      = @analysis.name
    %p
      %b Description:
      = @analysis.description
    %p
      %b User:
      = @analysis.user_id
    %p
      %b Geo region:
      = @analysis.geo_region_id
    %p
      %b Resolution:
      = @analysis.resolution_mi
    %p
      %b Analysis result:
      = @analysis.analysis_result_id

    = link_to 'Analyze', analyze_analysis_path(@analysis), method: :post
    \|
    = link_to 'Edit', edit_analysis_path(@analysis)
    \|
    = link_to 'Back', analyses_path
